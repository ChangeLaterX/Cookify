[flake8]
max-line-length = 110
extend-ignore = E203,W503,F401,F841,W293,F541,E402,F811,E741,E501
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    .mypy_cache,
    venv,
    .venv,
    migrations,
    htmlcov
per-file-ignores =
    __init__.py:F401,F403,F405
    tests/*:F401,F841,E501
    examples/*:E501,F401
